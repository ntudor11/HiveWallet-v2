    }
  },
  "dependencies": {
    "axios": "^0.21.2",
    "bootstrap": "^4.4.1",
    "chance": "^1.1.4",
    "chart.js": "^2.9.4",
    "coinkey": "^3.0.0",
    "doge-seed": "^1.0.0",
    "electron": "^13.6.6",
    "jwt-decode": "^2.2.0",
    "npm-check": "^5.9.0",
    "qr-encode": "^0.3.0",
    "randomstring": "^1.1.5",
    "react": "^16.11.0",
    "react-bootstrap": "^1.0.0-beta.16",
    "react-chartjs-2": "^2.8.0",
    "react-dom": "^16.11.0",
    "react-icons": "^3.8.0",
    "react-router": "^5.1.2",
